      </Fallback>
    </Item>
    <Item Type="Wheel">
      <Display X="100%" Y="100%" Width="50%" Height="33%" Anchor="BottomRight" />
    </Item>
    <Item Type="Box3D">
      <Display X="0%" Y="100%" Transparency="0.9" Width="45%" Height="60%" Anchor="BottomLeft" />
    </Item>
  </Drawings>
</ImageComposition>