<ImageComposition xsi:noNamespaceSchemaLocation="https://www.skraper.net/ImageComposition.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Information ShortName="Mix3 LD 4:3 No Fallback Text" LongName="3 Images Mix (No Fallback Text)" Description="3DBox + Screenshot (or title screenshot) + Wheel" Author="Jason Robitaille" />
